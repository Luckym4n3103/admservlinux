;
; BIND data file for local loopback interface
;
$TTL    604800
@       IN      SOA     l1m10-1.lan. root.l1m10-1.lan. (
                              1         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
@       IN      NS      l1m10-1.lan.
@       IN      A       192.168.15.40
@       IN      AAAA    ::1

